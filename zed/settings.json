{
  "theme": "Andromeda",
  "base_keymap": "VSCode",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 14,
  "vim_mode": true,
  "relative_line_numbers": true,
  "cursor_blink": false,
  "scrollbar": {
    "show": "never"
  },
  "buffer_line_height": {
    "custom": 1.7
  },
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font",
    "font_size": 14,
    "line_height": {
      "custom": 1.7
    }
  },
  "inlay_hints": {
    "enabled": false
  }
}
